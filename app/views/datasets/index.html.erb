<div class='container-fluid' id='data-browser-masthead'>
  <div class='darkener'></div>
  <div class='container'>
    <h2>Data Browser</h2>
  </div>
</div>
<div class='container'>
  <div class='row'>
    <div class='col-sm-12'>
      <%= @data_groups.each do |data_group| %>
        <%= data_group.inspect %>
      <% end %>

      <%= @datasets.each do |dataset| %>
        <%= dataset.inspect %>
      <% end %>
    </div>
  </div>
</div>