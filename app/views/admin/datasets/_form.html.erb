<div class='form-group'>
  <%= f.label :name, class: 'col-sm-2' %>
  <div class='col-sm-10'>
    <%= f.text_field :name %>
  </div>
</div>
<div class='form-group'>
  <%= f.label :source_description, class: 'col-sm-2' %>
  <div class='col-sm-10'>
    <%= f.text_area :source_description, size: '100x5' %>
  </div>
</div>
<div class='form-group'>
  <%= f.label :notes, class: 'col-sm-2' %>
  <div class='col-sm-10'>
    <%= f.text_area :notes, size: '100x5' %>
  </div>
</div>
<div class='form-group'>
  <%= f.label :source_url, class: 'col-sm-2' %>
  <div class='col-sm-10'>
    <%= f.text_field :source_url %>
  </div>
</div>
<div class='form-group'>
  <%= f.label :visualization_iframe, class: 'col-sm-2' %>
  <div class='col-sm-10'>
    <%= f.text_field :visualization_iframe %>
  </div>
</div>
<%= dataset.visualization_iframe.html_safe if dataset.visualization_iframe %>
<br/>